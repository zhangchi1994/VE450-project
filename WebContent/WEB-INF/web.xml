<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<servlet>
		<servlet-name>ActionServlet</servlet-name>
		<servlet-class>ve450.ruix.ActionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-class>ve450.ruix.UploadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadInfoServlet</servlet-name>
		<servlet-class>ve450.ruix.ReadInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GotoServlet</servlet-name>
		<servlet-class>ve450.ruix.GotoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SaveIMGServlet</servlet-name>
		<servlet-class>ve450.ruix.SaveIMGServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActionServlet</servlet-name>
		<url-pattern>/ActionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UploadServlet</servlet-name>
		<url-pattern>/UploadServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadInfoServlet</servlet-name>
		<url-pattern>/ReadInfoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GotoServlet</servlet-name>
		<url-pattern>/GotoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SaveIMGServlet</servlet-name>
		<url-pattern>/SaveIMGServlet</url-pattern>
	</servlet-mapping>

</web-app>